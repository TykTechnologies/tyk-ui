{
  "name": "@tyk-technologies/tyk-ui",
  "version": "2.2.35",
  "description": "Tyk UI - ui reusable components",
  "main": "lib/index.js",
  "scripts": {
    "start": "npx styleguidist server",
    "build-prod": "webpack",
    "build": "npx --max_old_space_size=4096 styleguidist build",
    "test": "npm run cy:run",
    "lint": "eslint ./src",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "cy:open": "cypress open --component",
    "cy:run": "cypress run --component"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TykTechnologies/tyk-ui.git"
  },
  "keywords": [
    "ui",
    "components"
  ],
  "author": "Tyk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TykTechnologies/tyk-ui/issues"
  },
  "homepage": "https://tyktechnologies.github.io/tyk-ui-styleguide/",
  "publishConfig": {
    "access": "restricted"
  },
  "nyc": {
    "all": true,
    "include": "src/**/*.js",
    "exclude": [
      "src/hooks",
      "src/utils",
      "src/form/formik",
      "src/form/redux-form"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/eslint-parser": "7.19.1",
    "@babel/preset-env": "7.19.4",
    "@babel/preset-react": "7.18.6",
    "@cypress/code-coverage": "3.10.0",
    "@cypress/react": "6.2.0",
    "@cypress/webpack-dev-server": "2.4.1",
    "@jsdevtools/coverage-istanbul-loader": "3.0.5",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "cypress": "10.11.0",
    "esbuild-loader": "2.20.0",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-webpack-plugin": "3.2.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.1",
    "monaco-editor-webpack-plugin": "7.0.1",
    "react-styleguidist": "13.0.0",
    "resolve-url-loader": "5.0.0",
    "sass-loader": "13.1.0",
    "style-loader": "3.3.1",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1"
  },
  "dependencies": {
    "@monaco-editor/react": "4.4.6",
    "brace": "0.11.1",
    "date-fns": "2.29.3",
    "echarts": "5.4.0",
    "flatpickr": "4.6.13",
    "immutable": "4.1.0",
    "lodash": "4.17.21",
    "monaco-editor": "0.34.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-ace": "6.6.0",
    "react-datetime": "3.1.1",
    "react-datetime-picker": "4.0.1",
    "react-docgen": "5.4.3",
    "react-dom": "18.2.0",
    "react-fast-compare": "3.2.0",
    "react-transition-group": "4.4.5",
    "sass": "1.55.0"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  }
}
